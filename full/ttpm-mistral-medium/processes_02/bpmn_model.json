{
  "$type": "bpmn:Definitions",
  "id": "HiringProcess-Definitions-id-0001",
  "targetNamespace": "http://bpmn.io/schema/bpmn",
  "exporter": "BPMN Modeler",
  "exporterVersion": "1.0",
  "rootElements": [
    {
      "$type": "bpmn:Process",
      "id": "HiringProcess-id-0001",
      "isExecutable": true,
      "flowElements": [
        {
          "$type": "bpmn:StartEvent",
          "id": "StartEvent-HiringNeedIdentified-id-0001",
          "name": "Hiring Need Identified"
        },
        {
          "$type": "bpmn:UserTask",
          "id": "UserTask-IdentifyHiringNeed-id-0001",
          "name": "Identify Hiring Need",
          "description": "Department identifies and documents hiring requirement."
        },
        {
          "$type": "bpmn:UserTask",
          "id": "UserTask-CreateJobDescription-id-0002",
          "name": "Create Job Description",
          "description": "HR creates detailed job posting with requirements."
        },
        {
          "$type": "bpmn:ServiceTask",
          "id": "ServiceTask-PostJob-id-0001",
          "name": "Post Job",
          "description": "System posts job to all configured job boards."
        },
        {
          "$type": "bpmn:UserTask",
          "id": "UserTask-ScreenResumes-id-0003",
          "name": "Screen Resumes",
          "description": "HR evaluates resumes against job requirements."
        },
        {
          "$type": "bpmn:ExclusiveGateway",
          "id": "ExclusiveGateway-QualifiedCandidates-id-0001",
          "name": "Qualified Candidates?",
          "gatewayDirection": "Diverging"
        },
        {
          "$type": "bpmn:UserTask",
          "id": "UserTask-ConductPhoneInterviews-id-0004",
          "name": "Conduct Phone Interviews",
          "description": "HR conducts initial phone screening with candidates."
        },
        {
          "$type": "bpmn:ExclusiveGateway",
          "id": "ExclusiveGateway-TopCandidates-id-0002",
          "name": "Top Candidates?",
          "gatewayDirection": "Diverging"
        },
        {
          "$type": "bpmn:UserTask",
          "id": "UserTask-ScheduleInterviews-id-0005",
          "name": "Schedule Interviews",
          "description": "HR coordinates interview schedules with candidates."
        },
        {
          "$type": "bpmn:UserTask",
          "id": "UserTask-ConductInPersonVirtualInterviews-id-0006",
          "name": "Conduct In-Person/Virtual Interviews",
          "description": "Interview panel evaluates top candidates."
        },
        {
          "$type": "bpmn:ExclusiveGateway",
          "id": "ExclusiveGateway-SelectedCandidate-id-0003",
          "name": "Selected Candidate?",
          "gatewayDirection": "Diverging"
        },
        {
          "$type": "bpmn:UserTask",
          "id": "UserTask-ExtendOffer-id-0007",
          "name": "Extend Offer",
          "description": "HR extends formal job offer to selected candidate."
        },
        {
          "$type": "bpmn:UserTask",
          "id": "UserTask-NegotiateSalary-id-0008",
          "name": "Negotiate Salary",
          "description": "HR negotiates compensation package with candidate."
        },
        {
          "$type": "bpmn:UserTask",
          "id": "UserTask-CompleteOnboarding-id-0009",
          "name": "Complete Onboarding",
          "description": "HR completes all onboarding procedures."
        },
        {
          "$type": "bpmn:EndEvent",
          "id": "EndEvent-HiringProcessComplete-id-0001",
          "name": "Hiring Process Complete",
          "description": "Process concludes with successful hire."
        },
        {
          "$type": "bpmn:EndEvent",
          "id": "EndEvent-NoSuitableCandidates-id-0002",
          "name": "No Suitable Candidates",
          "description": "Process concludes with no hire made."
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-StartToIdentifyNeed-id-0001",
          "sourceRef": "StartEvent-HiringNeedIdentified-id-0001",
          "targetRef": "UserTask-IdentifyHiringNeed-id-0001"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-IdentifyNeedToCreateJob-id-0002",
          "sourceRef": "UserTask-IdentifyHiringNeed-id-0001",
          "targetRef": "UserTask-CreateJobDescription-id-0002"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-CreateJobToPostJob-id-0003",
          "sourceRef": "UserTask-CreateJobDescription-id-0002",
          "targetRef": "ServiceTask-PostJob-id-0001"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-PostJobToScreenResumes-id-0004",
          "sourceRef": "ServiceTask-PostJob-id-0001",
          "targetRef": "UserTask-ScreenResumes-id-0003"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-ScreenResumesToQualifiedCheck-id-0005",
          "sourceRef": "UserTask-ScreenResumes-id-0003",
          "targetRef": "ExclusiveGateway-QualifiedCandidates-id-0001"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-QualifiedCandidatesToPhoneInterviews-id-0006",
          "name": "qualified_candidates > 0",
          "sourceRef": "ExclusiveGateway-QualifiedCandidates-id-0001",
          "targetRef": "UserTask-ConductPhoneInterviews-id-0004",
          "conditionExpression": {
            "$type": "bpmn:FormalExpression",
            "body": "qualified_candidates > 0"
          }
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-QualifiedCandidatesToNoCandidates-id-0007",
          "name": "qualified_candidates == 0 (Default)",
          "sourceRef": "ExclusiveGateway-QualifiedCandidates-id-0001",
          "targetRef": "EndEvent-NoSuitableCandidates-id-0002",
          "isDefault": true
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-PhoneInterviewsToTopCandidatesCheck-id-0008",
          "sourceRef": "UserTask-ConductPhoneInterviews-id-0004",
          "targetRef": "ExclusiveGateway-TopCandidates-id-0002"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-TopCandidatesToScheduleInterviews-id-0009",
          "name": "top_candidates > 0",
          "sourceRef": "ExclusiveGateway-TopCandidates-id-0002",
          "targetRef": "UserTask-ScheduleInterviews-id-0005",
          "conditionExpression": {
            "$type": "bpmn:FormalExpression",
            "body": "top_candidates > 0"
          }
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-TopCandidatesToNoCandidates-id-0010",
          "name": "top_candidates == 0 (Default)",
          "sourceRef": "ExclusiveGateway-TopCandidates-id-0002",
          "targetRef": "EndEvent-NoSuitableCandidates-id-0002",
          "isDefault": true
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-ScheduleInterviewsToConductInterviews-id-0011",
          "sourceRef": "UserTask-ScheduleInterviews-id-0005",
          "targetRef": "UserTask-ConductInPersonVirtualInterviews-id-0006"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-ConductInterviewsToSelectedCheck-id-0012",
          "sourceRef": "UserTask-ConductInPersonVirtualInterviews-id-0006",
          "targetRef": "ExclusiveGateway-SelectedCandidate-id-0003"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-SelectedCandidateToExtendOffer-id-0013",
          "name": "selected_candidate == true",
          "sourceRef": "ExclusiveGateway-SelectedCandidate-id-0003",
          "targetRef": "UserTask-ExtendOffer-id-0007",
          "conditionExpression": {
            "$type": "bpmn:FormalExpression",
            "body": "selected_candidate == true"
          }
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-SelectedCandidateToNoCandidates-id-0014",
          "name": "selected_candidate == false (Default)",
          "sourceRef": "ExclusiveGateway-SelectedCandidate-id-0003",
          "targetRef": "EndEvent-NoSuitableCandidates-id-0002",
          "isDefault": true
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-ExtendOfferToNegotiateSalary-id-0015",
          "sourceRef": "UserTask-ExtendOffer-id-0007",
          "targetRef": "UserTask-NegotiateSalary-id-0008"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-NegotiateSalaryToCompleteOnboarding-id-0016",
          "sourceRef": "UserTask-NegotiateSalary-id-0008",
          "targetRef": "UserTask-CompleteOnboarding-id-0009"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "SequenceFlow-CompleteOnboardingToEnd-id-0017",
          "sourceRef": "UserTask-CompleteOnboarding-id-0009",
          "targetRef": "EndEvent-HiringProcessComplete-id-0001"
        }
      ]
    }
  ]
}