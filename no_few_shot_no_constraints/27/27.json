          "outgoing": ["SequenceFlow-0014"]
        },
        {
          "id": "SendTask-0004",
          "name": "Send: Late Report to insurance provider",
          "incoming": ["SequenceFlow-0014"],
          "outgoing": ["SequenceFlow-0015"]
        },
        {
          "id": "EndEvent-0003",
          "name": "Process Complete",
          "incoming": ["SequenceFlow-0015"]
        }
