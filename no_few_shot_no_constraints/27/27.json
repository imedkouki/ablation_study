        {
          "id": "UserTask-0002",
          "name": "Notify employer immediately",
          "incoming": ["SequenceFlow-0007"],
          "outgoing": ["SequenceFlow-0009"]
        },
        {
          "id": "SendTask-0001",
          "name": "Send: Report to Labour Inspectorate",
          "incoming": ["SequenceFlow-0009"],
          "outgoing": ["SequenceFlow-0010"]
        },
        {
          "id": "SendTask-0002",
          "name": "Send: Report to insurance provider",
          "incoming": ["SequenceFlow-0010"],
          "outgoing": ["SequenceFlow-0011"]
        },
        {
          "id": "UserTask-0003",
          "name": "Notify employer within 5 days",
          "incoming": ["SequenceFlow-0008"],
          "outgoing": ["SequenceFlow-0012"]
        },
        {
          "id": "SendTask-0003",
          "name": "Send: Report to insurance provider",
          "incoming": ["SequenceFlow-0012"],
          "outgoing": ["SequenceFlow-0013"]
        },
        {
          "id": "EndEvent-0001",
          "name": "Process Complete",
          "incoming": ["SequenceFlow-0011", "SequenceFlow-0013"]
        },
        {
          "id": "EndEvent-0002",
          "name": "No Action Required",
          "incoming": ["SequenceFlow-0005"]
        },
        {
          "id": "BoundaryEvent-0001",
          "name": "5 days",
