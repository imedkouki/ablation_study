              }
            },
            {
              "bpmn:ServiceTask": {
                "id": "ServiceTask-0001",
                "name": "Check transaction data request"
              }
            },
            {
              "bpmn:ExclusiveGateway": {
                "id": "ExclusiveGateway-0001",
                "name": "Request valid?"
              }
            },
            {
              "bpmn:ServiceTask": {
                "id": "ServiceTask-0002",
                "name": "Transmit data"
              }
            },
            {
              "bpmn:EndEvent": {
                "id": "EndEvent-0001",
                "name": "Process completed"
              }
            },
            {
              "bpmn:ServiceTask": {
                "id": "ServiceTask-0003",
                "name": "Reject request"
              }
            },
            {
              "bpmn:SequenceFlow": {
                "id": "SequenceFlow-0001",
                "sourceRef": "StartEvent-0001",
                "targetRef": "ServiceTask-0001"
