          "targetRef": "UserTask_006"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "Flow_011",
          "sourceRef": "UserTask_006",
          "targetRef": "UserTask_007"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "Flow_012",
          "sourceRef": "UserTask_007",
          "targetRef": "ReceiveTask_001"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "Flow_013",
          "sourceRef": "ReceiveTask_001",
          "targetRef": "Gateway_003"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "Flow_014",
          "name": "Approved",
          "sourceRef": "Gateway_003",
          "targetRef": "UserTask_008"
        },
        {
          "$type": "bpmn:SequenceFlow",
          "id": "Flow_015",
          "name": "Rejected",
          "sourceRef": "Gateway_003",
          "targetRef": "UserTask_009"
        },
